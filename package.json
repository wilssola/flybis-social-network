{
  "name": "flybis",
  "version": "1.0.0",
  "description": "[![Gitpod Ready-to-Code](https://img.shields.io/badge/Gitpod-Ready--to--Code-blue?logo=gitpod)](https://gitpod.io/#https://github.com/WolfTheZelda/Flybis)",
  "main": "main.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "npm link",
    "deploy": "flybis web-build && firebase deploy",
    "deploy-web": "flybis web-build && firebase deploy --only hosting",
    "deploy-functions": "firebase deploy --only functions",
    "android-run-cache": "flutter run --profile --cache-sksl --purge-persistent-cache",
    "android-build-apk": "flutter build apk --release --tree-shake-icons --shrink --obfuscate --split-per-abi --split-debug-info=./.debug_info/android/",
    "android-build-appbundle": "flutter build appbundle --target-platform android-arm, android-arm64, android-x64 --release --tree-shake-icons --shrink --obfuscate --split-debug-info=./.debug_info/android/",
    "flutter-launcher-icons": "flutter pub run flutter_launcher_icons:main",
    "flutter-native-splash": "flutter pub run flutter_native_splash:create",
    "import-sorter": "flutter pub run import_sorter:main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WolfTheZelda/Flybis.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WolfTheZelda/Flybis/issues"
  },
  "homepage": "https://github.com/WolfTheZelda/Flybis",
  "dependencies": {
    "commander": "^7.2.0",
    "copy-dir": "^1.3.0",
    "fs-extra": "^9.1.0",
    "sri-calc": "^1.0.9",
    "terser": "^5.3.3"
  },
  "bin": {
    "flybis": "main.js"
  }
}
