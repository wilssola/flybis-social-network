rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if request.auth != null || resource.contentType.matches('image/jpeg') || resource.contentType.matches('video/mp2ts') || resource.contentType.matches('audio/x-mpegurl');
      allow write: if request.auth != null;
    }
  }
}
